<body class="no-skin">
    <div class="Loading" style="display:none">
        <button class="btn btn-primary" type="button" disabled="">
            <span class="spinner-border spinner-border-sm mr-1" role="status" aria-hidden="true"></span>
            Loading...
        </button>
    </div>
    <div id="navbar" class="navbar navbar-default    navbar-collapse       h-navbar">
        <script type="text/javascript">
            try { ace.settings.check('navbar', 'fixed') } catch (e) { }
        </script>

        <div class="navbar-container" id="navbar-container">
            <div class="navbar-header pull-left">
                <a href="#" class="navbar-brand">
                    <small>
                        <i class="fa fa-leaf"></i>
                        Cosmic Game
                    </small>
                </a>
            </div>

            <div class="navbar-buttons navbar-header pull-right  collapse navbar-collapse" role="navigation">
                <ul class="nav ace-nav">
                    <li class="light-blue user-min">
                        <a data-toggle="dropdown" href="#" class="dropdown-toggle">
                            <small class="username"></small>

                            @*<img class="nav-user-photo" src="~/dist/avatars/user.jpg" alt="Jason's Photo" />*@
                            <span class="user-info">
                                <small>Welcome,</small>
                                <span class="username"></span>
                            </span>

                            <i class="ace-icon fa fa-caret-down"></i>
                        </a>

                        <ul class="user-menu dropdown-menu-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">

                            @*<li>
                                    <a href="profile.html">
                                        <i class="ace-icon fa fa-user"></i>
                                        Profile
                                    </a>
                                </li>*@

                            @*                            <li class="divider"></li>*@

                            <li>
                                <a class="cursor-pointer" onclick="logout()">
                                    <i class="ace-icon fa fa-power-off"></i>
                                    Logout
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div><!-- /.navbar-container -->
    </div>

    <div class="main-container" id="main-container">
        <script type="text/javascript">
            try { ace.settings.check('main-container', 'fixed') } catch (e) { }
        </script>

        <div id="sidebar" class="sidebar      h-sidebar                navbar-collapse collapse">
            <script type="text/javascript">
                try { ace.settings.check('sidebar', 'fixed') } catch (e) { }
            </script>

            <ul class="nav nav-list">
                <li class="hover">
                    <a href="/client/dashboard">
                        <i class="menu-icon fa fa-tachometer"></i>
                        <span class="menu-text"> Dashboard </span>
                    </a>

                    <b class="arrow"></b>
                </li>
                <li class="hover">
                    <a href="/client/ChartHolder">
                        <i class="menu-icon fa fa-users"></i>
                        <span class="menu-text"> Regular Chart Holders </span>
                    </a>

                    <b class="arrow"></b>
                </li>
                <li class="hover">
                    <a href="#" class="dropdown-toggle">
                        <i class="menu-icon fa fa-list"></i>
                        <span class="menu-text"> Books Master </span>

                        <b class="arrow fa fa-angle-down"></b>
                    </a>

                    <b class="arrow"></b>

                    <ul class="submenu">
                        <li class="hover">
                            <a href="/client/books">
                                <i class="menu-icon fa fa-caret-right"></i>
                                Books
                            </a>

                            <b class="arrow"></b>
                        </li>

                        <li class="hover">
                            <a href="/client/Books/Chapter">
                                <i class="menu-icon fa fa-caret-right"></i>
                                Chapter
                            </a>

                            <b class="arrow"></b>
                        </li>
                        <li class="hover">
                            <a href="/client/Books/Contents">
                                <i class="menu-icon fa fa-caret-right"></i>
                                Contents
                            </a>

                            <b class="arrow"></b>
                        </li>
                    </ul>
                </li>
                <li class="hover">
                    <a href="/client/Books/BookView">
                        <i class="menu-icon fa fa-users"></i>
                        <span class="menu-text"> Book View </span>
                    </a>

                    <b class="arrow"></b>
                </li>
            </ul><!-- /.nav-list -->

            <script type="text/javascript">
                try { ace.settings.check('sidebar', 'collapsed') } catch (e) { }
            </script>
        </div>
        <div class="main-content">
            <div class="main-content-inner">
                <div class="page-content">
                    @RenderBody()
                </div><!-- /.page-content -->
            </div>
        </div><!-- /.main-content -->

        <div class="footer">
            <div class="footer-inner">
                <div class="footer-content">
                    <span class="bigger-120">
                        <span class="blue bolder">Cosmic</span>
                        Game
                    </span>
                </div>
            </div>
        </div>

        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
        </a>
    </div><!-- /.main-container -->
    <div id="ChartViewPopup"></div>
