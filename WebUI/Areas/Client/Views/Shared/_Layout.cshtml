    <script src="~/dist/js/ace.min.js"></script>
    <script src="~/dist/js/chosen.jquery.min.js"></script>
    <script src="~/dist/js/date-time/bootstrap-datepicker.min.js"></script>
    <script src="~/dist/js/date-time/bootstrap-timepicker.min.js"></script>
    <script src="~/dist/js/date-time/moment.min.js"></script>
    <script src="~/dist/js/date-time/daterangepicker.min.js"></script>
    <script src="~/dist/js/date-time/bootstrap-datetimepicker.min.js"></script>

    <!-- inline scripts related to this page -->
    <script src="~/lib/jquery-toast-plugin/jquery.toast.min.js"></script>
    <script src="~/js/SystemJs/formToJson.min.js"></script>
    <script src="~/js/SystemJs/jquery.tmpl.min.js"></script>
    <script src="~/js/SystemJs/Caller.js?v=@GlobalVars.siteVersion"></script>
    <script src="~/js/SystemJs/Common.js?v=@GlobalVars.siteVersion"></script>
    <script src="~/js/Page/layout.js?v=@GlobalVars.siteVersion"></script>

